<Project Sdk="Godot.NET.Sdk/3.3.0">
	<PropertyGroup>
		<TargetFramework>net472</TargetFramework>
		<RootNamespace>GodotManager</RootNamespace>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Godot.NET.Sdk" Version="4.1.1" />
		<PackageReference Include="GodotSharpExtras" Version="0.3.6" />
		<PackageReference Include="Newtonsoft.JSON" Version="13.0.3" />
		<PackageReference Include="SixLabors.ImageSharp" Version="2.1.6" />
		<PackageReference Include="System.IO.Compression" Version="4.3.0" />
	</ItemGroup>
	<PropertyGroup Condition="true">
		<NoWarn>0414</NoWarn>
	</PropertyGroup>
	<ItemGroup>
		<None Include="**/*.tscn" />
		<None Include="**/*.tres" />
		<None Include="**/*.svg" />
		<None Include="**/*.png" />
		<None Include="**/*.ico" />
		<None Include="**/*.ttf" />
		<None Include="**/*.cfg" />
	</ItemGroup>
</Project>
